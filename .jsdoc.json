{
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "include": ["."],
    "exclude": ["node_modules", "rethinkdb_data", ".git", ".vscode", "old_src"]
  },
  "recurseDepth": 10,
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "docdash": {
    "search": true
  }
}
