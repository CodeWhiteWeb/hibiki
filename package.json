{
  "name": "hibiki",
  "version": "3.2.1-alpha",
  "description": "A versatile and feature-packed Discord bot written in JavaScript.",
  "author": "smolespi <espi@lesbian.codes> (https://lesbian.codes)",
  "contributors": [
    "resolved <resolvedxd@gmail.com> (https://github.com/resolvedxd)"
  ],
  "license": "AGPL-3.0-or-later",
  "homepage": "https://hibiki.app",
  "main": "src/index.js",
  "bugs": {
    "url": "https://github.com/smolespi/Hibiki/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smolespi/Hibiki.git"
  },
  "engines": {
    "npm": ">=6.9.0",
    "node": ">=12.0.0"
  },
  "scripts": {
    "dev": "nodemon",
    "docs": "jsdoc -c .jsdoc.json",
    "lint": "eslint --fix .",
    "postinstall": "npx link-module-alias",
    "start": "cross-env NODE_ENV=production node src/index.js",
    "setup": "node src/scripts/setup.js",
    "test": "eslint ."
  },
  "_moduleAliases": {
    "commands": "src/commands",
    "dashboard": "src/extensions/dashboard",
    "events": "src/events",
    "extensions": "src/extensions",
    "root": ".",
    "scripts": "src/scripts",
    "structures": "src/structures",
    "utils": "src/utils"
  },
  "dependencies": {
    "@sentry/node": "^5.18.0",
    "body-parser": "^1.19.0",
    "ejs": "^3.1.3",
    "eris": "^0.13.3",
    "eris-additions": "^1.4.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "node-fetch": "^3.0.0-beta.7",
    "passport": "^0.4.1",
    "passport-discord": "^0.1.4",
    "rethinkdb-ts": "^2.4.2"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.3.2",
    "erlpack": "^0.1.3",
    "eventemitter3": "^4.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "docdash": "^1.2.0",
    "eslint": "^7.3.1",
    "jsdoc": "^3.6.4",
    "link-module-alias": "^1.2.0",
    "nodemon": "^2.0.4"
  }
}
